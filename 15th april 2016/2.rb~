#!/usr/bin/ruby -w

class Gadgets
    def phones
        puts "phones are usually cheap";
    end
    def laptops
        puts "laptops are easy to carry";
    end
    def computers
        puts "computers are very durable";
    end
end
newobjects=Gadgets.new;
newobjects.phones;
newobjects.laptops;
newobjects.computers;       
 
